+bool menuAtLeastOneActionWithText(const QMenu* menu, const QString& text)
+{
+    Q_FOREACH(auto a, menu->actions())
+    {
+        if (a->text() == text) {
+            return true;
+        }
+    }
+
+    return false;
+}
